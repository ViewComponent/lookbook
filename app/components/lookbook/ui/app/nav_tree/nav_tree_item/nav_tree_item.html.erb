<%= render component_tag(:sl_tree_item,
  href: url_path,
  key: lookup_path,
  x: {
    data: "navTreeItem({keywords: #{search_terms.to_json}, collection: #{collection?.to_json}})",
    show: "!filteredOut",
    cloak: true,
    "@click": "itemClicked"
  }
) do %>
  <sl-icon name="<%= icon %>"></sl-icon>
  <%= label %>
  <% children&.each do |node| %>
    <%= lookbook_nav_tree_item(node) %>
  <% end %>
<% end %>
