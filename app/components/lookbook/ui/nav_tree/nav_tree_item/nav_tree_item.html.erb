<%= render component_tag(:sl_tree_item,
  href: nav_path,
  key: lookup_path,
  x: {
    "data": "navTreeItem",
    "@click": "itemClicked",
  },
  "nav-tree-index": (index? ? true : nil)
) do %>
  <sl-icon name="<%= icon %>"></sl-icon>
  <%= label %>
  <% children&.each do |node| %>
    <%= lb_nav_tree_item(node) %>
  <% end %>
<% end %>
