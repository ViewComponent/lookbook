<%=
  lookbook_preview_inspector(@preview, @target) do |layout|
    @inspector.preview_panels.each do |panel|
      layout.with_preview_panel(panel) do
        render panel.partial
      end
    end

    @inspector.drawer_panels.each do |panel|
      layout.with_drawer_panel(panel) do
        render panel.partial
      end
    end
  end
%>